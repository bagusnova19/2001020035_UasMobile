        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ichimori.simpletodoapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "df86a88f-1343-4602-a014-e42f5d28ec84"
      }
    },
    "owner": "ichimori"
  }
